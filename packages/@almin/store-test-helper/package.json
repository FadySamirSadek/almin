{
  "name": "@almin/store-test-helper",
  "version": "1.0.0",
  "description": "Create Store helper for testing.",
  "keywords": [
    "almin",
    "testing"
  ],
  "homepage": "https://github.com/almin/almin/tree/master/packages/@almin/store-test-helper/",
  "bugs": {
    "url": "https://github.com/almin/almin/issues"
  },
  "license": "MIT",
  "author": "azu",
  "files": [
    "bin/",
    "lib/",
    "src/"
  ],
  "main": "lib/store-test-helper.js",
  "types": "lib/store-test-helper.d.ts",
  "directories": {
    "lib": "lib",
    "test": "test"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/almin/almin.git"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production tsc -p .",
    "prepublish": "npm run --if-present build",
    "test": "mocha \"test/**/*.ts\"",
    "prettier": "prettier --write \"**/*.{js,jsx,ts,tsx,css}\"",
    "watch": "tsc -p . --watch",
    "clean": "rimraf lib/",
    "ci": "npm run test"
  },
  "prettier": {
    "printWidth": 120,
    "singleQuote": false,
    "tabWidth": 4
  },
  "dependencies": {
    "almin": "^0.16.0"
  },
  "devDependencies": {
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "cross-env": "^5.1.6",
    "mocha": "^5.2.0",
    "prettier": "^1.13.4",
    "rimraf": "^2.6.2",
    "ts-node": "^6.1.0",
    "ts-node-test-register": "^3.0.0",
    "typescript": "^2.9.1"
  },
  "publishConfig": {
    "access": "public"
  }
}
